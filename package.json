{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "rm -rf public/build && vite",
        "build": "rm -rf public/build public/hot && vite build",
        "browser": "echo \"const supportedBrowsers = $(browserslist-useragent-regexp --allowHigherVersions);\n\nexport { supportedBrowsers };\" > resources/ts/browser.ts",
        "all:fix": "pnpm run prettier:fix ; pnpm run eslint:fix",
        "eslint": "eslint",
        "eslint:fix": "eslint --fix",
        "prettier": "prettier --check resources/**/*.{css,scss,vue,js,cjs,ts,json,md} *.{css,scss,vue,js,cjs,ts,json,md}",
        "prettier:fix": "prettier --write resources/**/*.{css,scss,vue,js,cjs,ts,json,md} *.{css,scss,vue,js,cjs,ts,json,md}",
        "test": "vitest run --reporter=verbose",
        "test:watch": "vitest --reporter=verbose --watch",
        "test:coverage": "vitest run --reporter=html --coverage",
        "test:coverage:watch": "vitest --reporter=html --coverage --watch",
        "test:browser": "vitest run --reporter=html --coverage --browser",
        "test:browser:watch": "vitest --reporter=html --coverage --browser --watch",
        "test:browser:debug": "vitest --reporter=html --coverage --browser --watch --debug"
    },
    "dependencies": {
        "@eslint/js": "^9.35.0",
        "@nuxt/ui": "^3.3.3",
        "@primevue/core": "^4.3.9",
        "@primevue/icons": "^4.3.9",
        "@sentry/vue": "^10.10.0",
        "@tailwindcss/vite": "^4.1.13",
        "@unhead/vue": "^2.0.14",
        "@vitejs/plugin-vue": "^6.0.1",
        "autoprefixer": "^10.4.21",
        "axios": "^1.11.0",
        "concurrently": "^9.2.1",
        "laravel-echo": "^2.2.0",
        "laravel-vite-plugin": "^2.0.1",
        "lodash": "^4.17.21",
        "pinia": "^3.0.3",
        "postcss": "^8.5.6",
        "postcss-convert-values": "^7.0.7",
        "primeicons": "^7.0.0",
        "primevue": "^4.3.9",
        "pusher-js": "8.4.0",
        "sass": "^1.92.1",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.13",
        "tailwindcss-primeui": "^0.6.1",
        "vite": "^7.1.5",
        "vite-plugin-static-copy": "^3.1.2",
        "vue": "^3.5.21",
        "vue-clipboard3": "^2.0.0",
        "vue-router": "^4.5.1"
    },
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.10",
        "@vue/test-utils": "^2.4.6",
        "browserslist-useragent-regexp": "^4.1.3",
        "eslint": "^9.35.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-vue": "^9.33.0",
        "globals": "^16.3.0",
        "jsdom": "^26.1.0",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.43.0",
        "vitest": "^3.2.4",
        "volt-vue": "0.0.0-alpha.4"
    }
}
